#!/usr/bin/env bash

set -euo pipefail

export BUILD_DIR="$1"

cat $BUILD_DIR/requirements.txt | sed -E "s/^torch==([[:digit:]]\.[[:digit:]]\.[[:digit:]]);.+/https:\/\/download.pytorch.org\/whl\/cpu\/torch-\1%2Bcpu-cp39-cp39-linux_x86_64.whl/; /^    --hash=sha256:.+/d" | sed -E 's/ \\$//' > requirements.txt
